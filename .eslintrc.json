{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard", "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint", "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
}
